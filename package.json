{
  "name": "mcp-gdocs",
  "version": "1.0.0",
  "description": "MCP server for Google Docs editing via Claude Mobile",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "setup": "npx ts-node setup/oauth-setup.ts",
    "deploy": "npm run build && npx cdk deploy --require-approval never",
    "synth": "npm run build && npx cdk synth",
    "destroy": "npx cdk destroy",
    "lint": "npx biome check src/ infra/",
    "lint:fix": "npx biome check --write src/ infra/",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "google-docs",
    "claude",
    "aws-lambda",
    "anthropic"
  ],
  "author": "tranhoangan22",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tranhoangan22/mcp-gdocs.git"
  },
  "homepage": "https://github.com/tranhoangan22/mcp-gdocs#readme",
  "bugs": {
    "url": "https://github.com/tranhoangan22/mcp-gdocs/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@aws-sdk/client-secrets-manager": "^3.478.0",
    "googleapis": "^129.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.10",
    "@types/aws-lambda": "^8.10.131",
    "@types/node": "^20.10.5",
    "aws-cdk": "^2.118.0",
    "aws-cdk-lib": "^2.118.0",
    "constructs": "^10.3.0",
    "esbuild": "^0.27.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
